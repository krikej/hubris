// Interface to RNG.

Interface(
    name: "Rng",
    ops: {
        "fill": (
            doc: "Fill buffer with entropy from platform RNG.",
            leases: {
                "source": (type: "[u8]", write: true),
            },
            reply: Result(
                ok: "usize",
                err: CLike("RngError"),
            ),
        ),
    }
)
